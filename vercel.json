{
  "framework": "nextjs",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Expose-Headers", "value": "X-PAYMENT,X-PAYMENT-RESPONSE,PAYMENT-REQUIRED,PAYMENT-RESPONSE" }
      ]
    }
  ]
}
